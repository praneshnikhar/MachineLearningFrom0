   "source": [
    "import tensorflow as tf\n",
    "def loss(output, y):\n",
    "    dot_product =  y * tf.log(output)\n",
    "    xentropy = -tf.reduce_sum(dot_product, reduction_indices=1)\n",
    "    loss = tf.reduce_mean(xentropy)\n",
    "    return loss"
   ]
