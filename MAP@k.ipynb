    "def mapk(y_true,y_pred, k):\n",
    "    apk_values=[]\n",
    "    for i in range(len(y_true)):\n",
    "        apk_values.append(apk(y_true[i], y_pred[i], k))\n",
    "    return sum(apk_values)/len(apk_values)"
